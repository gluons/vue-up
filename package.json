{
  "name": "@gluons/vue-up",
  "version": "0.0.1",
  "description": "Bundle Vue.js library via Rollup.",
  "main": "./dist/bundle.js",
  "typings": "./dist/bundle.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=6.9.0"
  },
  "scripts": {
    "prebuild": "rimraf dist/*",
    "build": "tsc -p .",
    "watch": "tsc -p . -w",
    "pretest": "yarn build && ts-node ./scripts/run.ts",
    "test": "jest -c jest.config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluons/vue-up.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "library",
    "bundler",
    "packager",
    "rollup"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/vue-up/issues"
  },
  "homepage": "https://github.com/gluons/vue-up",
  "peerDependencies": {
    "vue": "2"
  },
  "dependencies": {
    "@gluons/rollup-plugin-postcss-only": "^0.1.0",
    "chalk": "^2.4.1",
    "cssnano": "^4.0.5",
    "del": "^3.0.0",
    "figures": "^2.0.0",
    "log-update": "^2.3.0",
    "moren": "^1.1.0",
    "node-sass": "^4.9.3",
    "nvl": "^1.1.1",
    "postcss": "^7.0.2",
    "pug": "^2.0.3",
    "ramda": "^0.25.0",
    "rollup": "^0.64.0",
    "rollup-plugin-babel-minify": "^5.0.0",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-typescript2": "^0.16.1",
    "rollup-plugin-vue": "^4.3.2",
    "vue-template-compiler": "^2.5.17"
  },
  "devDependencies": {
    "@types/del": "^3.0.1",
    "@types/jest": "^23.3.1",
    "@types/node": "8",
    "@types/ora": "^1.3.4",
    "@types/signale": "^1.2.0",
    "@vue/test-utils": "^1.0.0-beta.24",
    "autoprefixer": "^9.1.0",
    "babel-core": "^6.26.3",
    "jest": "^23.5.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "tslint-config-gluons": "^2.0.0",
    "typescript": "~3.0.0",
    "vue": "^2.5.17",
    "vue-class-component": "^6.2.0"
  }
}
