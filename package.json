{
  "name": "@gluons/vue-up",
  "version": "0.7.0",
  "description": "Bundle Vue.js library via Rollup.",
  "main": "./dist/bundle.js",
  "typings": "./dist/bundle.d.ts",
  "bin": "./bin/vue-up",
  "files": [
    "bin",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "rimraf dist/*",
    "build": "tsc",
    "lint": "tslint -p .",
    "format": "prettier-tslint fix \"{{src,scripts}/**/*.ts,test/*.ts}\"",
    "pretest": "npm run build && ts-node ./scripts/prepareTest.ts",
    "test": "npm run lint && jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluons/vue-up.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "library",
    "bundler",
    "packager",
    "rollup"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/vue-up/issues"
  },
  "homepage": "https://gluons.github.io/vue-up/",
  "peerDependencies": {
    "vue": "2",
    "vue-template-compiler": "2"
  },
  "dependencies": {
    "@gluons/rollup-plugin-postcss-only": "^2.0.0",
    "@gluons/vue-pack-dev": "^1.20.2",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.0",
    "@sindresorhus/slugify": "^0.11.0",
    "chalk": "^3.0.0",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "figures": "^3.1.0",
    "joycon": "^2.2.5",
    "joycon-yaml-loader": "^2.0.2",
    "lodash.isnil": "^4.0.0",
    "lodash.pick": "^4.4.0",
    "log-update": "^3.3.0",
    "moren": "^3.0.0",
    "node-sass": "^4.13.0",
    "nvl": "^1.2.2",
    "ow": "^0.16.0",
    "postcss": "^7.0.26",
    "pug": "^2.0.4",
    "rollup": "^1.28.0",
    "rollup-plugin-babel-minify": "^9.1.1",
    "rollup-plugin-resolve-alias": "^0.2.1",
    "rollup-plugin-typescript2": "^0.26.0",
    "rollup-plugin-vue": "<=5.1.6",
    "tslib": "^1.10.0",
    "validate-npm-package-name": "^3.0.0",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@gluons/prettier-config": "^2.0.0",
    "@types/cheerio": "^0.22.15",
    "@types/cross-spawn": "^6.0.1",
    "@types/glob": "^7.1.1",
    "@types/jest": "^25.0.0",
    "@types/lodash.isnil": "^4.0.6",
    "@types/lodash.pick": "^4.4.6",
    "@types/node": "10",
    "@types/signale": "^1.2.1",
    "@types/yargs": "^15.0.0",
    "@vue/server-test-utils": "^1.0.0-beta.30",
    "@vue/test-utils": "^1.0.0-beta.30",
    "autoprefixer": "^9.7.3",
    "babel-jest": "^25.0.0",
    "cheerio": "^1.0.0-rc.3",
    "cross-spawn": "^7.0.1",
    "jest": "^25.0.0",
    "prettier": "^1.19.1",
    "prettier-tslint": "^0.4.2",
    "rimraf": "^3.0.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-config-gluons": "^2.1.0",
    "typescript": "^3.7.4",
    "vue": "^2.6.11",
    "vue-class-component": "^7.1.0",
    "vue-server-renderer": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "optionalDependencies": {
    "joycon-ts-loader": "^3.0.1"
  }
}
