{
  "name": "@gluons/vue-up",
  "version": "0.4.1",
  "description": "Bundle Vue.js library via Rollup.",
  "main": "./dist/bundle.js",
  "typings": "./dist/bundle.d.ts",
  "bin": "./bin/vue-up",
  "files": [
    "bin",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "rimraf dist/*",
    "build": "tsc",
    "lint": "tslint -p .",
    "format": "prettier-tslint fix \"{{src,scripts}/**/*.ts,test/*.ts}\"",
    "pretest": "npm run build && ts-node ./scripts/prepareTest.ts",
    "test": "npm run lint && jest -c jest.config.json --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluons/vue-up.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "library",
    "bundler",
    "packager",
    "rollup"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/vue-up/issues"
  },
  "homepage": "https://gluons.github.io/vue-up/",
  "peerDependencies": {
    "vue": "2",
    "vue-template-compiler": "2"
  },
  "dependencies": {
    "@gluons/rollup-plugin-postcss-only": "^1.2.0",
    "@gluons/vue-pack-dev": "^1.17.0",
    "@sindresorhus/slugify": "^0.8.0",
    "chalk": "^2.4.2",
    "cssnano": "^4.1.10",
    "del": "^3.0.0",
    "figures": "^2.0.0",
    "joycon": "^2.2.4",
    "joycon-yaml-loader": "^2.0.1",
    "lodash.isnil": "^4.0.0",
    "lodash.pick": "^4.4.0",
    "log-update": "^2.3.0",
    "moren": "^2.0.1",
    "node-sass": "^4.11.0",
    "nvl": "^1.2.2",
    "ow": "^0.12.0",
    "postcss": "^7.0.14",
    "pug": "^2.0.3",
    "rollup": "^1.2.0",
    "rollup-plugin-babel-minify": "^7.0.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-resolve-alias": "^0.2.1",
    "rollup-plugin-typescript2": "^0.19.0",
    "rollup-plugin-vue": "^4.7.1",
    "tslib": "^1.9.3",
    "validate-npm-package-name": "^3.0.0",
    "yargs": "^13.0.0"
  },
  "devDependencies": {
    "@gluons/prettier-config": "^2.0.0",
    "@types/cheerio": "^0.22.10",
    "@types/cross-spawn": "^6.0.0",
    "@types/del": "^3.0.1",
    "@types/jest": "^24.0.5",
    "@types/lodash.isnil": "^4.0.5",
    "@types/lodash.pick": "^4.4.5",
    "@types/node": "8",
    "@types/signale": "^1.2.0",
    "@types/yargs": "^12.0.9",
    "@vue/server-test-utils": "^1.0.0-beta.29",
    "@vue/test-utils": "^1.0.0-beta.29",
    "autoprefixer": "^9.4.7",
    "babel-core": "^6.26.3",
    "cheerio": "^1.0.0-rc.2",
    "cross-spawn": "^6.0.5",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "prettier-tslint": "^0.4.2",
    "rimraf": "^2.6.3",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-gluons": "^2.1.0",
    "typescript": "~3.3.3",
    "vue": "^2.6.6",
    "vue-class-component": "^6.3.2",
    "vue-server-renderer": "^2.6.6",
    "vue-template-compiler": "^2.6.6"
  },
  "optionalDependencies": {
    "joycon-ts-loader": "^3.0.0"
  }
}
