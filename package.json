{
  "name": "@gluons/vue-up",
  "version": "0.0.1",
  "description": "A Vue.js library bundler.",
  "main": "./dist/bundle.js",
  "typings": "./dist/bundle.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist/*",
    "build": "tsc -p .",
    "watch": "tsc -p . -w",
    "pretest": "yarn build && ts-node ./scripts/run.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluons/vue-up.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "library",
    "bundler",
    "packager",
    "rollup"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/vue-up/issues"
  },
  "homepage": "https://github.com/gluons/vue-up",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.jest.json"
      }
    }
  },
  "peerDependencies": {
    "vue": "2"
  },
  "dependencies": {
    "del": "^3.0.0",
    "node-sass": "^4.9.0",
    "nvl": "^1.1.1",
    "ora": "^3.0.0",
    "postcss": "^7.0.0",
    "pug": "^2.0.3",
    "rollup": "^0.63.0",
    "rollup-plugin-babel-minify": "^5.0.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-css-only": "^0.4.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-typescript2": "^0.16.1",
    "rollup-plugin-vue": "^4.1.4",
    "vue-template-compiler": "^2.5.16"
  },
  "devDependencies": {
    "@types/del": "^3.0.1",
    "@types/jest": "^23.0.0",
    "@types/node": "8",
    "@types/ora": "^1.3.4",
    "@vue/test-utils": "^1.0.0-beta.16",
    "babel-core": "^6.26.3",
    "jest": "^23.0.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.0.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "tslint-config-gluons": "^1.0.5",
    "typescript": "~2.8",
    "vue": "^2.5.16",
    "vue-class-component": "^6.2.0"
  }
}
