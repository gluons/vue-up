{
  "name": "@gluons/vue-up",
  "version": "0.0.1",
  "description": "Bundle Vue.js library via Rollup.",
  "main": "./dist/bundle.js",
  "typings": "./dist/bundle.d.ts",
  "bin": "./bin/vue-up",
  "files": [
    "bin",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=6.11.5"
  },
  "scripts": {
    "prepublishOnly": "yarn build",
    "prebuild": "rimraf dist/*",
    "build": "tsc",
    "lint": "tslint -p .",
    "pretest": "yarn build && ts-node ./scripts/prepareTest.ts",
    "test": "yarn lint && jest -c jest.config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluons/vue-up.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "library",
    "bundler",
    "packager",
    "rollup"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/vue-up/issues"
  },
  "homepage": "https://gluons.github.io/vue-up/",
  "peerDependencies": {
    "vue": "2"
  },
  "dependencies": {
    "@gluons/rollup-plugin-postcss-only": "^0.1.0",
    "@gluons/vue-pack-dev": "^1.2.7",
    "@sindresorhus/slugify": "^0.3.0",
    "chalk": "^2.4.1",
    "cssnano": "^4.0.5",
    "del": "^3.0.0",
    "figures": "^2.0.0",
    "joycon": "^2.0.0",
    "joycon-yaml-loader": "^2.0.1",
    "log-update": "^2.3.0",
    "moren": "^1.1.0",
    "node-sass": "^4.9.3",
    "nvl": "^1.1.1",
    "ow": "^0.6.0",
    "postcss": "^7.0.2",
    "pug": "^2.0.3",
    "ramda": "^0.25.0",
    "rollup": "^0.65.0",
    "rollup-plugin-babel-minify": "^5.0.0",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-resolve-alias": "^0.2.0",
    "rollup-plugin-typescript2": "^0.17.0",
    "rollup-plugin-vue": "^4.3.2",
    "vue-template-compiler": "^2.5.17",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.0",
    "@types/del": "^3.0.1",
    "@types/jest": "^23.3.1",
    "@types/node": "8",
    "@types/ora": "^1.3.4",
    "@types/signale": "^1.2.0",
    "@types/yargs": "^11.1.1",
    "@vue/test-utils": "^1.0.0-beta.24",
    "autoprefixer": "^9.1.3",
    "babel-core": "^6.26.3",
    "cross-spawn": "^6.0.5",
    "jest": "^23.5.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.1.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.10.0",
    "tslint-config-gluons": "^2.0.0",
    "typescript": "~3.0.0",
    "vue": "^2.5.17",
    "vue-class-component": "^6.2.0"
  },
  "optionalDependencies": {
    "joycon-ts-loader": "^2.0.0"
  }
}
